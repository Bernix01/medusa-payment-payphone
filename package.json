{
  "name": "medusa-payment-payphone",
  "version": "0.0.1",
  "description": "Payphone payment provider",
  "author": "bernix01 <gbernal096@gmail.com>",
  "license": "MIT",
  "keywords": [
    "storage",
    "plugin",
    "medusa-plugin",
    "medusa-plugin-storage",
    "typescript",
    "supabase",
    "medusa"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernix01/medusa-payment-payphone.git#main"
  },
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && tsc -p tsconfig.server.json",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.20.1"
  },
  "dependencies": {
    "@medusajs/admin": "7.1.10"
  },
  "devDependencies": {
    "@medusajs/medusa": "^1.20.1",
    "@medusajs/medusa-cli": "^1.3.21",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
